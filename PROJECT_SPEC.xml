<?xml version="1.0" encoding="UTF-8"?>
<project>
  <metadata>
    <name>AuditCaseOS</name>
    <description>Internal audit case management system with AI-powered analysis, evidence vault, and smart report generation</description>
    <version>0.1.0</version>
    <repository>https://github.com/SBG4/auditcaseos</repository>
    <last_updated>2026-01-14</last_updated>
    <current_phase>1</current_phase>
    <overall_progress>phase_1_complete</overall_progress>
  </metadata>

  <architecture>
    <stack>
      <backend>FastAPI (Python 3.12)</backend>
      <database>PostgreSQL 16 + pgvector</database>
      <storage>MinIO (S3-compatible)</storage>
      <ai>Ollama (local LLM)</ai>
      <ocr>Paperless-ngx (Phase 2)</ocr>
      <frontend>React (Phase 3)</frontend>
      <collaboration>Nextcloud + ONLYOFFICE (Phase 3)</collaboration>
    </stack>
    <ports>
      <api>18000</api>
      <postgres>15432</postgres>
      <minio_api>19000</minio_api>
      <minio_console>19001</minio_console>
      <ollama>21434</ollama>
      <paperless>18080</paperless>
      <frontend>13000</frontend>
    </ports>
  </architecture>

  <case_id_system>
    <format>SCOPE-TYPE-SEQ</format>
    <example>FIN-USB-0001</example>
    <scopes>
      <scope code="FIN">Finance</scope>
      <scope code="HR">Human Resources</scope>
      <scope code="IT">Information Technology</scope>
      <scope code="SEC">Security</scope>
      <scope code="OPS">Operations</scope>
      <scope code="CORP">Corporate</scope>
      <scope code="LEGAL">Legal</scope>
      <scope code="RND">Research and Development</scope>
      <scope code="LEG">Legal</scope>
      <scope code="PRO">Procurement</scope>
      <scope code="MKT">Marketing</scope>
      <scope code="QA">Quality Assurance</scope>
      <scope code="ENV">Environmental</scope>
      <scope code="SAF">Health and Safety</scope>
      <scope code="EXT">External</scope>
      <scope code="GOV">Governance</scope>
      <scope code="GEN">General</scope>
    </scopes>
    <types>
      <type>USB</type>
      <type>EMAIL</type>
      <type>WEB</type>
      <type>POLICY</type>
    </types>
  </case_id_system>

